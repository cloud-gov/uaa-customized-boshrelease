{
    "rules": {
        "indent": [
            2,
            4
        ],
        "quotes": [
            0,
            "single"
        ],
        "linebreak-style": [
            2,
            "unix"
        ],
        "semi": [
            2,
            "always"
        ]
    },
    "globals": {
      "validatePassword": true
    },
    "env": {
        "browser": true,
        "jquery": true
    },
    "extends": "eslint:recommended"
}
